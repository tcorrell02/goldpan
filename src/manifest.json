{
  "manifest_version": 3,
  "name": "Goldpan: Job Sifter",
  "version": "1.0.0",
  "content_scripts": [
    {
      "js": ["src/content/main.ts"],
      "matches": ["https://www.linkedin.com/jobs/*"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["https://www.linkedin.com/*"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://www.linkedin.com/*"
  ]
}